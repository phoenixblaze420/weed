{
  "name": "ganja-guru",
  "version": "1.0.0",
  "description": "The ultimate AI-powered cannabis ecosystem with AR/VR, 3D Printing, AI/ML, and e-commerce integration.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint .",
    "deploy": "firebase deploy",
    "3dpod": "node src/3dprint.js",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "secure": "npm audit --production",
    "start:prod": "NODE_ENV=production node src/server.js",
    "predeploy": "npm run lint && npm run test",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "serve": "http-server ./dist",
    "clean": "rimraf dist && mkdir dist",
    "watch": "webpack --watch",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "jsdoc -c jsdoc.json",
    "commit": "git-cz",
    "release": "standard-version",
    "start:debug": "node --inspect src/server.js",
    "fix": "eslint . --fix",
    "prepare": "husky install",
    "serve:ar": "aframe serve",
    "ai:train": "node src/ai/train.js",
    "ai:test": "node src/ai/test.js",
    "stripe:webhook": "stripe listen --forward-to localhost:8080/webhook",
    "prod:check": "npm audit && npm outdated",
    "e2e": "cypress open",
    "e2e:run": "cypress run --headless",
    "docker:build": "docker build -t ganja-app .",
    "docker:run": "docker run -p 8080:8080 ganja-app",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "update": "npm-check-updates -u && npm install",
    "start:local": "dotenv -e .env.local -- node src/server.js",
    "env:check": "dotenv -e .env -- printenv"
  },
  "author": "phoenixblaze420",
  "license": "MIT",
  "dependencies": {
  "express": "^4.18.2",
  "dotenv": "^16.0.3",
  "firebase": "^9.6.1",
  "three": "^0.153.0",
  "a-frame": "^1.3.0",
  "openai": "^3.2.1",
  "langchain": "^0.0.88",
  "tensorflow": "^2.13.0",
  "react": "^18.2.0",
  "next": "^13.4.0",
  "stripe": "^11.0.0",
  "mongoose": "^7.2.1",
  "socket.io": "^4.6.1",
  "body-parser": "^1.20.2",
  "chart.js": "^4.0.1",
  "babylonjs": "^5.8.0",
  "d3": "^7.8.0",
  "react-three-fiber": "^9.6.5",
  "expo": "^49.0.0",
  "gpt-3-encoder": "^1.1.0",
  "face-api.js": "^0.23.0",
  "pose-detection": "^1.1.0",
  "deeplearn": "^1.5.3",
  "pytorch": "^2.0.1",
  "keras": "^2.13.1",
  "robotjs": "^0.7.0",
  "shopify-api": "^4.0.0",
  "woocommerce-api": "^1.5.0",
  "clerk-js": "^4.2.0",
  "aws-sdk": "^3.0.0",
  "cloudinary": "^1.37.1",
  "web3": "^1.10.1",
  "ethers": "^5.7.0",
  "apollo-client": "^3.8.0",
  "graphql": "^16.7.0",
  "tailwindcss": "^3.3.0",
  "postcss": "^8.4.21",
  "autoprefixer": "^10.4.14",
  "framer-motion": "^6.5.5",
  "gsap": "^3.12.2",
  "prisma": "^4.15.0",
  "next-auth": "^4.22.1",
  "supabase": "^1.44.0",
  "tesseract.js": "^4.1.1",
  "opencv.js": "^1.0.0",
  "speech-to-text": "^0.2.1",
  "node-red": "^3.1.1",
  "heroku": "^7.63.0",
  "vercel": "^5.0.1",
  "vite": "^4.3.9"
}
  "devDependencies": {
    "nodemon": "^2.0.22",
    "webpack": "^5.88.1",
    "jest": "^29.6.2",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "weed",
    "hustle",
    "blaze",
    "420",
    "web",
    "AR",
    "VR",
    "3D Printing",
    "PoD",
    "AI",
    "ML",
    "LLM",
    "NLP",
    "e-commerce",
    "cannabis",
    "real-time",
    "stripe",
    "ganja"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "port": "8080",
    "env": "development"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/phoenixblaze420"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
