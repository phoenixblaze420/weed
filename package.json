{
  "name": "ganja-guru",
  "version": "1.0.0",
  "description": "The ultimate AI-powered cannabis ecosystem with AR/VR, 3D Printing, AI/ML, and e-commerce integration.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint .",
    "deploy": "firebase deploy",
    "3dpod": "node src/3dprint.js",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "secure": "npm audit --production",
    "start:prod": "NODE_ENV=production node src/server.js",
    "predeploy": "npm run lint && npm run test",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "serve": "http-server ./dist",
    "clean": "rimraf dist && mkdir dist",
    "watch": "webpack --watch",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "jsdoc -c jsdoc.json",
    "commit": "git-cz",
    "release": "standard-version",
    "start:debug": "node --inspect src/server.js",
    "fix": "eslint . --fix",
    "prepare": "husky install",
    "serve:ar": "aframe serve",
    "ai:train": "node src/ai/train.js",
    "ai:test": "node src/ai/test.js",
    "stripe:webhook": "stripe listen --forward-to localhost:8080/webhook",
    "prod:check": "npm audit && npm outdated",
    "e2e": "cypress open",
    "e2e:run": "cypress run --headless",
    "docker:build": "docker build -t ganja-app .",
    "docker:run": "docker run -p 8080:8080 ganja-app",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "update": "npm-check-updates -u && npm install",
    "start:local": "dotenv -e .env.local -- node src/server.js",
    "env:check": "dotenv -e .env -- printenv"
  },
  "author": "phoenixblaze420",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "firebase": "^9.6.1",
    "three": "^0.153.0",
    "a-frame": "^1.3.0",
    "openai": "^3.2.1",
    "langchain": "^0.0.88",
    "tensorflow": "^2.13.0",
    "react": "^18.2.0",
    "next": "^13.4.0",
    "stripe": "^11.0.0",
    "mongoose": "^7.2.1",
    "socket.io": "^4.6.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "webpack": "^5.88.1",
    "jest": "^29.6.2",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "weed",
    "hustle",
    "blaze",
    "420",
    "web",
    "AR",
    "VR",
    "3D Printing",
    "PoD",
    "AI",
    "ML",
    "LLM",
    "NLP",
    "e-commerce",
    "cannabis",
    "real-time",
    "stripe",
    "ganja"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "port": "8080",
    "env": "development"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/phoenixblaze420"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
